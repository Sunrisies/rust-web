{
  "openapi": "3.1.0",
  "info": {
    "title": "Rust Web API",
    "description": "一个简单的Rust web API",
    "termsOfService": "https://www.rust-web-api.com/terms",
    "contact": {
      "name": "Sunrisies",
      "url": "https://github.com/Sunrisies/rust-web",
      "email": "3266420686@qq.com"
    },
    "license": {
      "name": ""
    },
    "version": "1.0"
  },
  "paths": {
    "/api/auth/register": {
      "post": {
        "tags": [
          "鉴权模块"
        ],
        "operationId": "用户注册",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterResponse"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "注册成功",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CommonResponse_UserDto"
                }
              }
            }
          },
          "400": {
            "description": "验证错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CommonResponse_Option_UserDto"
                }
              }
            }
          },
          "409": {
            "description": "用户名已存在",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CommonResponse_Option_UserDto"
                }
              }
            }
          },
          "500": {
            "description": "服务器内部错误",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SimpleRespData"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CommonResponse_Option_UserDto": {
        "type": "object",
        "required": [
          "code",
          "message"
        ],
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "data": {
            "oneOf": [
              {
                "type": "null"
              },
              {
                "type": "object",
                "required": [
                  "user_name",
                  "pass_word"
                ],
                "properties": {
                  "email": {
                    "type": [
                      "string",
                      "null"
                    ]
                  },
                  "pass_word": {
                    "type": "string"
                  },
                  "user_name": {
                    "type": "string"
                  }
                }
              }
            ]
          },
          "message": {
            "type": "string"
          }
        }
      },
      "CommonResponse_UserDto": {
        "type": "object",
        "required": [
          "code",
          "message"
        ],
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32",
            "minimum": 0
          },
          "data": {
            "type": "object",
            "required": [
              "user_name",
              "pass_word"
            ],
            "properties": {
              "email": {
                "type": [
                  "string",
                  "null"
                ]
              },
              "pass_word": {
                "type": "string"
              },
              "user_name": {
                "type": "string"
              }
            }
          },
          "message": {
            "type": "string"
          }
        }
      },
      "RegisterResponse": {
        "type": "object",
        "required": [
          "user_name",
          "pass_word"
        ],
        "properties": {
          "pass_word": {
            "type": "string"
          },
          "user_name": {
            "type": "string"
          }
        }
      },
      "SimpleRespData": {
        "type": "object",
        "required": [
          "data",
          "message"
        ],
        "properties": {
          "data": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "UserDto": {
        "type": "object",
        "required": [
          "user_name",
          "pass_word"
        ],
        "properties": {
          "email": {
            "type": [
              "string",
              "null"
            ]
          },
          "pass_word": {
            "type": "string"
          },
          "user_name": {
            "type": "string"
          }
        }
      }
    }
  }
}
